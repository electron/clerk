{
  "name": "electron-release-clerk",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^19.0.4",
    "probot": "^12.2.8"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/jest": "^29.0.3",
    "@types/node": "^16.11.7",
    "debug": "^4.3.4",
    "husky": "^4.2.5",
    "jest": "^29.0.3",
    "lint-staged": "^8.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\"",
    "postinstall": "tsc",
    "start": "DEBUG=* probot run ./lib/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
